{
  "name": "gis-bridge-explorer",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "dev": "turbo run db:up dev",
    "db:kill": "docker-compose down -v"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.0.0",
    "turbo": "latest",
    "typescript": "^5.0.0"
  }
}
